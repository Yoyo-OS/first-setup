#: data/org.yoyoos.FirstSetup.desktop.in:3
msgid "Yoyo OS First Setup"
msgstr "Первоначальная настройка Yoyo OS"

#: data/org.yoyoos.FirstSetup.appdata.xml.in:7
msgid "Tweak your Yoyo OS installation."
msgstr "Настройте свою установку Yoyo OS."

#: data/org.yoyoos.FirstSetup.appdata.xml.in:10
msgid "No description"
msgstr "Нет описания"

#: data/org.yoyoos.FirstSetup.appdata.xml.in:26
msgid "First release"
msgstr "Первый выпуск"

#: yoyo_first_setup/gtk/default-conn-check.ui:12
#: yoyo_first_setup/defaults/conn_check.py:75
msgid "Checking Connection…"
msgstr "Проверка соединения…"

#: yoyo_first_setup/gtk/default-conn-check.ui:13
#: yoyo_first_setup/defaults/conn_check.py:76
msgid "Please wait until the connection check is done."
msgstr "Пожалуйста, подождите, пока не будет выполнена проверка подключения."

#: yoyo_first_setup/gtk/default-theme.ui:17
msgid "Default"
msgstr "По умолчанию"

#: yoyo_first_setup/gtk/default-theme.ui:29
msgid "Dark"
msgstr "Тёмный"

